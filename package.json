{
  "name": "cross-spawn-cb",
  "version": "2.0.13",
  "description": "Cross spawn with a completion callback",
  "keywords": [
    "cross-spawn",
    "callbacks",
    "callback"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/cross-spawn-cb.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "buffer-v6-polyfill": "^1.0.5",
    "call-once-fn": "^1.0.5",
    "core-js": "^3.39.0",
    "cross-spawn": "^7.0.6",
    "function-exec-sync": "^1.1.12",
    "next-tick": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "is-version": "^0.2.1",
    "node-install-release": "^1.8.1",
    "node-resolve-versions": "^1.0.5",
    "node-version-utils": "^1.2.1",
    "pinkie-promise": "^2.0.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
