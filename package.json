{
  "name": "cross-spawn-cb",
  "version": "1.5.7",
  "description": "Cross spawn with a completion callback",
  "keywords": [
    "cross-spawn",
    "callbacks",
    "callback"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/cross-spawn-cb.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "buffer-v6-polyfill": "^1.0.5",
    "call-once-fn": "^1.0.1",
    "core-js": "^3.39.0",
    "cross-spawn": "^7.0.6",
    "function-exec-sync": "^1.1.5",
    "next-tick": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "is-version": "^0.2.1",
    "node-install-release": "^1.6.3",
    "node-resolve-versions": "^1.0.5",
    "node-version-utils": "^1.1.3"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.cjs",
    "targets": [
      "cjs"
    ]
  }
}
