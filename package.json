    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "cr": "^0.1.0",
    "is-version": "^1.0.7",
    "node-install-release": "^1.16.2",
    "node-resolve-versions": "^1.3.11",
    "node-version-use": "^2.1.5",
    "node-version-utils": "^1.3.15",
    "pinkie-promise": "^2.0.1",
    "ts-dev-stack": "^1.21.3",
    "tsds-config": "^0.2.1"
